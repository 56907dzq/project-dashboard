#!/usr/bin/env node

require('commander')
  .version(require('../package.json').version)
  .usage('<command> [options]')
  .command('init', '初始化项目')
  .command('start', '启动调试')
  .command('create', '创建模板')
  .command('update', '更新noahv框架')
  .command('test', '运行单测框架')
  .command('upgrade', '更新脚手架工具')
  .parse(process.argv)
